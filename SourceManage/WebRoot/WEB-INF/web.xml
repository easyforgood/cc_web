<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<display-name></display-name>



	<!-- Source Upload Part -->
	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>SourceUploadServlet</servlet-name>
		<servlet-class>Servlet.SourceUploadServlet</servlet-class>
	</servlet>

	<!-- Source Search Part -->
	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>SourceSearchServlet</servlet-name>
		<servlet-class>Servlet.SourceSearchServlet</servlet-class>
	</servlet>

	<!-- Source Delete Part -->
	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>CourseDeleteServlet</servlet-name>
		<servlet-class>Servlet.CourseDeleteServlet</servlet-class>
	</servlet>


	<!-- Course Manage Part -->
	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>CourseManageServlet</servlet-name>
		<servlet-class>Servlet.CourseManageServlet</servlet-class>
	</servlet>

	<!-- Course update Part -->
	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>CourseUpdateServlet</servlet-name>
		<servlet-class>Servlet.CourseUpdateServlet</servlet-class>
	</servlet>

	<!-- Course File manage Part -->
	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>CourseFileManageServlet</servlet-name>
		<servlet-class>Servlet.CourseFileHomeManageServlet</servlet-class>
	</servlet>

<!-- File manage Part -->
	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>CourseFileManage</servlet-name>
		<servlet-class>Servlet.CourseFileManageServlet</servlet-class>
	</servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FileDeleteServlet</servlet-name>
    <servlet-class>Servlet.FileDeleteServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FileUpdateServlet</servlet-name>
    <servlet-class>Servlet.FileUpdateServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SearchServlet</servlet-name>
    <servlet-class>Servlet.SearchServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Px_CourseServlet</servlet-name>
    <servlet-class>Servlet.Px_CourseServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>testAjax</servlet-name>
    <servlet-class>Servlet.testAjax</servlet-class>
  </servlet>











	<!-- Filter Part -->
	<filter>
		<filter-name>log</filter-name>
		<filter-class>Filter.LogFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>log</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>





	<servlet-mapping>
		<servlet-name>SourceUploadServlet</servlet-name>
		<url-pattern>/servlet/SourceUploadServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SourceSearchServlet</servlet-name>
		<url-pattern>/servlet/SourceSearchServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CourseDeleteServlet</servlet-name>
		<url-pattern>/servlet/CourseDeleteServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CourseManageServlet</servlet-name>
		<url-pattern>/servlet/CourseManageServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CourseUpdateServlet</servlet-name>
		<url-pattern>/servlet/CourseUpdateServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CourseFileManageServlet</servlet-name>
		<url-pattern>/servlet/CourseFileHomeManageServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CourseFileManage</servlet-name>
		<url-pattern>/servlet/CourseFileManageServlet</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileDeleteServlet</servlet-name>
    <url-pattern>/servlet/FileDeleteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileUpdateServlet</servlet-name>
    <url-pattern>/servlet/FileUpdateServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchServlet</servlet-name>
    <url-pattern>/servlet/SearchServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Px_CourseServlet</servlet-name>
    <url-pattern>/px/Px_CourseServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>testAjax</servlet-name>
    <url-pattern>/servlet/testAjax</url-pattern>
  </servlet-mapping>



	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
